<Window x:Class="configurator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:configurator"
        mc:Ignorable="d"
        Title="Dr. Ajectory Configurator" Height="400" Width="500" MaxHeight="400" MaxWidth="500">
    <TabControl>
        <TabItem Header="Dr. Ajectory Settings">
            <local:SettingsTab></local:SettingsTab>
        </TabItem>
        <TabItem Header="Manual Override" IsEnabled="False" />
        <TabItem Header="Update from GitHub" IsEnabled="False" />
        <TabItem Header="About" IsEnabled="False" />

    </TabControl>
</Window>
